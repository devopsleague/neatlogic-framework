	<typeAliases>
		<typeAlias type="codedriver.framework.dao.typehandler.XssHandler" alias="XssHandler" />
	</typeAliases>
	<!-- <typeHandlers> <typeHandler jdbcType="VARCHAR" javaType="String" handler="codedriver.framework.dao.typehandler.XssHandler" /> </typeHandlers> -->
</configuration>
